{
  "name": "vault-client",
  "version": "0.1.0",
  "description": "Interactively visualize your crates.io dependencies.",
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "clean": "gatsby clean",
    "develop": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint src --ext .tsx,.ts && stylelint \"src/**/*.scss\"",
    "lint:fix": "eslint src --ext .tsx,.ts && stylelint \"src/**/*.scss\"",
    "preload-fonts": "gatsby-preload-fonts",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "antd": "^4.8.4",
    "axios": "^0.21.0",
    "gatsby": "^2.26.1",
    "gatsby-plugin-manifest": "^2.6.1",
    "gatsby-plugin-preload-fonts": "^1.3.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-sass": "^2.5.1",
    "gatsby-plugin-typescript": "^2.6.0",
    "gatsby-plugin-use-query-params": "^1.0.1",
    "node-sass": "^4.14.1",
    "qs": "^6.9.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-force-graph": "^1.37.1",
    "react-helmet": "^6.1.0",
    "typescript": "^4.0.5",
    "use-query-params": "^1.1.9"
  },
  "devDependencies": {
    "@hack4impact-uiuc/eslint-plugin": "^2.0.4",
    "@types/loadable__component": "^5.13.1",
    "@types/qs": "^6.9.5",
    "@types/react-helmet": "^6.1.0",
    "eslint": "^7.13.0",
    "prettier": "^2.1.2",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arpanlaha/vault.git"
  }
}
