{
  "name": "vault-client",
  "version": "0.1.0",
  "description": "Interactively visualize your crates.io dependencies.",
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint src --ext .tsx,.ts && stylelint \"src/**/*.scss\"",
    "lint:fix": "eslint src --ext .tsx,.ts && stylelint \"src/**/*.scss\"",
    "preload-fonts": "gatsby-preload-fonts",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@loadable/component": "^5.13.1",
    "antd": "^4.6.3",
    "axios": "^0.20.0",
    "gatsby": "^2.24.57",
    "gatsby-plugin-manifest": "^2.4.28",
    "gatsby-plugin-preload-fonts": "^1.2.24",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-typescript": "^2.4.19",
    "gatsby-plugin-use-query-params": "^1.0.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-force-graph": "^1.36.9",
    "react-helmet": "^6.1.0",
    "typescript": "^4.0.2",
    "use-query-params": "^1.1.6"
  },
  "devDependencies": {
    "@hack4impact-uiuc/eslint-plugin": "^2.0.4",
    "@types/loadable__component": "^5.13.0",
    "@types/react-helmet": "^6.1.0",
    "eslint": "^7.8.1",
    "markdownlint-cli": "^0.23.2",
    "prettier": "^2.1.1",
    "stylelint": "^13.7.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arpanlaha/vault.git"
  }
}
