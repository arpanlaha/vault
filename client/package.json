{
  "name": "vault-client",
  "version": "0.1.0",
  "description": "Interactively visualize your crates.io dependencies.",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint src --ext .tsx,.ts && stylelint \"src/**/*.scss\"",
    "lint:fix": "eslint src --ext .tsx,.ts && stylelint \"src/**/*.scss\"",
    "serve": "serve -s build",
    "start": "react-scripts start",
    "typecheck": "tsc"
  },
  "dependencies": {
    "antd": "^4.8.4",
    "axios": "^0.21.0",
    "node-sass": "^4.14.1",
    "qs": "^6.9.4",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-force-graph-3d": "^1.18.8",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "use-query-params": "^1.1.9"
  },
  "devDependencies": {
    "@hack4impact-uiuc/eslint-plugin": "^2.0.4",
    "@types/node": "^12.0.0",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.6",
    "eslint": "^7.13.0",
    "prettier": "^2.1.2",
    "serve": "^11.3.2",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arpanlaha/vault.git"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
